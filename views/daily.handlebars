{{!-- Daily card layout. --}}

<div class="contatiner  cardContainer ">
    <div class="row">

        {{!-- Add new entry card. --}}
        <div class="card mb-3 col-sm-12 col-lg-6 col-xl-4">
            <div class="plusButton">
                <span class="add">+</span>
            </div>
        </div>


        {{!-- Main daily card. --}}
        <div class="card mb-3 col-sm-12 col-lg-6 col-xl-4">

            <div class="card-header bg-transparent">
                <div class="dailyTimeRow">
                    <span>06:00</span> <span class="dateStamp"> 11451 </span> <span>10:30</span>
                </div>
            </div>

            <div class="card-body">
                <div class="contentSection">
                    <span class="contentSection">
                        <i class="fas fa-flag"></i>
                        <strong>Highlight:</strong> {{#each example}}{{/each}}
                    </span>
                    <br>
                    <span class="contentSection">
                        <i class="fas fa-arrow-up"></i>
                        <strong>Positive:</strong> goes here...
                    </span>
                    <br>
                    <span class="contentSection">
                        <i class="fas fa-arrow-down"></i>
                        <strong>Negative:</strong> goes here.
                    </span>
                    <br>
                </div>
            </div>

            <div class="card-footer habitsRow bg-transparent" >
                <div>
                    <span>
                        <i class="fas fa-running"></i>
                    </span>
                    <span>
                        <i class="fas fa-dumbbell"></i>
                    </span>
                    <span>
                        <i class="fas fa-tint"></i>
                    </span>
                </div>
            </div>
        </div>

    </div>
    <div class="row blog-container daily">

    </div>
</div>


        
